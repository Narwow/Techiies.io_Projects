<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cs499android.com.cppmapbox">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MarkerSelected" />
        <activity android:name=".NavigationActivity" />
        <activity android:name=".Admini"></activity>
        <activity android:name=".Buildings"></activity>
        <activity android:name=".Dorm"></activity>
        <activity android:name=".Food"></activity>
        <activity android:name=".Land"></activity>
        <activity android:name=".Park"></activity>
        <activity android:name=".Bathroom"></activity>
        <activity android:name=".Settings"></activity>
    </application>

</manifest>